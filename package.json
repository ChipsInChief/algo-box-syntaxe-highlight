{
  "name": "algo-box-syntax-highlighter",
  "displayName": "Algo{box} Syntax Highlighter üñç",
  "description": "Coloration syntaxique pour pseudo-code algorithmique (üá´üá∑) compatible avec Algobox.",
  "version": "0.0.1",
  "publisher": "ChipsInChief",
  "license": "MIT",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChipsInChief/algo-box-syntaxe-highlight"
  },
  "bugs": {
    "url": "https://github.com/ChipsInChief/algo-box-syntaxe-highlight/issues"
